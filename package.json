    "@typescript-eslint/parser": "6.4.0",
    "autoprefixer": "10.4.15",
    "clsx": "2.0.0",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.18",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.0",
    "postcss": "8.4.28",
    "prettier": "2.8.8",
